<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
<shape>
    <corners android:radius="5dp"/>
<gradient
    android:startColor="#4a6f5f"
    android:centerColor="#4a6f5f"
    android:centerY="0.75"
    android:endColor="#4a6f5f"
    android:angle="270"
    />
</shape>
    </item>
<item android:id="@android:id/progress">
<clip>
    <shape>
        <corners android:radius="5dp"/>
        <gradient
            android:startColor="#56e2a7"
            android:centerColor="#56e2a7"
            android:centerY="0.75"
            android:endColor="#56e2a7"
            android:angle="270"
            />
    </shape>
</clip>
</item>
    </layer-list>